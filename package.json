{
  "name": "frugaltesting_registration_automation_project",
  "version": "1.0.0",
  "main": "index.js",
  "description": "Complete Registration System with Automation Testing - Frugal Testing Assessment",
  "scripts": {
    "test": "cypress run",
    "test:open": "cypress open",
    "test:specific": "cypress run --spec 'automation/test.js'",
